# Application Configuration
NODE_ENV=development
API_KEY=YOUR_API_KEY_HERE
HOST_PORT=3030
ENABLE_LOGGER=true
APP_TIMEZONE=Asia/Jakarta

# Security Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3030

# Webhook Configuration
WEBHOOK_URL=https://example.com/webhook
WEBHOOK_EXCLUDE_ME=false
WEBHOOK_EXCLUDE_GROUP=false
WEBHOOK_EXCLUDE_CHANNEL=false

# Rate Limiting
RATE_LIMIT=100
RATE_LIMIT_EXPIRE=60

# Message Configuration
MESSAGE_DELAY_TYPE=fixed
MESSAGE_DELAY=1

# Queue Configuration
QUEUE_CONNECTION=cron

# Redis Configuration (if using Redis queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USER=
REDIS_PASSWORD=

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/whatsapp_api
MONGODB_USER=
MONGODB_PASSWORD=
MONGODB_DB=whatsapp_api
